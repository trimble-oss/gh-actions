{
  "version": "0.2",
  "words": [
    "GOROOT",
    "whitesource"
  ],
  "allowCompoundWords": true,
  "flagWords": [
    "hte"
  ],
  "language": "en,en-US",
  "ignoreRegExpList": [
    "<!-- prettier-ignore -->\\n(`{3,})\\w*\\n[\\s\\S]+?\\1",
    "\\[(\\*{2})?@\\w+?\\1\\]",
    "\\[`\\w+`\\]",
    "ve{2,}r{2,}y",
    "ve+r+y+long\\w*",
    "\\/.*?\\/",
    "\\_\\w+",
    "\\#\\w+"
  ],
  "ignorePaths": [
    ".cspell.json",
    "**/node_modules/**",
    "*.min.*",
    "{coverage,dist,.cache,.git,.vscode,.DS_Store,tests}/**/*",
    "*.{log,svg,snap}",
    "test.*"
  ],
  "useGitignore": true
}
